<%= form_for @contact, url: {action: "create"} do |f| %>
  <p>
    <%= f.label "Name" %><br>
    <%= f.text_field "name" %>
  </p>
  <p>
    <%= f.label "Email" %><br>
    <%= f.text_field "email" %>
  </p>
  <p>
    <%= f.label "Phone Number" %><br>
    <%= f.text_field "number" %>
  </p>

  <p>
    <%= f.submit "Save" %> 
  </p>
<% end %>